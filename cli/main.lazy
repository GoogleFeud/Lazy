
struct Person {
    name: str
    age: i8

    static create: fn(name: str, age: i8) -> Person {
        new Person { name, age };
    }

    age_up: fn() {
        self.age += 0x01;
    }

    next: fn() -> Vec<A>? {
        let counter = 0;
        Promise::interval(fn() {
            print(counter);
            counter += 1;
        }, 1m);
    }

}

